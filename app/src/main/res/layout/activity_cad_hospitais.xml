<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CadHospitais">


    <EditText
        android:id="@+id/txtRazaoHosp"
        android:layout_width="353dp"
        android:layout_height="58dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="32dp"
        android:ems="10"
        android:hint="UNIMED"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/txtCnpjHosp"
        android:layout_width="356dp"
        android:layout_height="65dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="115dp"
        android:ems="10"
        android:hint="12.145.789/0002-01"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/txtEndHosp"
        android:layout_width="352dp"
        android:layout_height="64dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="201dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="Rua J, 15, Alvorada 1" />

    <EditText
        android:id="@+id/txtTelHosp"
        android:layout_width="355dp"
        android:layout_height="61dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="154dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="(92)3012-4561" />

    <Button
        android:id="@+id/btnCadastrarHosp"
        android:layout_width="342dp"
        android:layout_height="61dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="39dp"
        android:text="Cadastrar Hospital" />



</RelativeLayout>