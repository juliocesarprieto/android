<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="visible"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imgClientes"
        android:layout_width="103dp"
        android:layout_height="110dp"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/imgHospitais"
        android:layout_marginStart="41dp"
        android:background="@drawable/cad_cliente"
        tools:onClick="clientes" />

    <ImageView
        android:id="@+id/imgHospitais"
        android:layout_width="106dp"
        android:layout_height="103dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="61dp"
        android:layout_marginTop="79dp"
        android:background="@drawable/hospitais" />

    <ImageView
        android:id="@+id/imgMarcarConsulta"
        android:layout_width="113dp"
        android:layout_height="107dp"
        android:layout_alignStart="@+id/imgClientes"
        android:layout_alignTop="@+id/imgMarcarExame"
        android:background="@drawable/marcar_consulta" />

    <ImageView
        android:id="@+id/imgMarcarExame"
        android:layout_width="110dp"
        android:layout_height="100dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="138dp"
        android:layout_marginEnd="50dp"
        android:background="@drawable/marcar_exame" />

</RelativeLayout>